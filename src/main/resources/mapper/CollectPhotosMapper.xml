<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.tencent.wxcloudrun.dao.CollectPhotosMapper">

    <resultMap id="objectMapper" type="com.tencent.wxcloudrun.model.CollectPhotos">
        <id property="id" column="id"/>
        <result property="name" column="name"/>
        <result property="collectId" column="collect_id"/>
        <result property="sort" column="sort"/>
        <result property="status" column="status"/>
        <result property="type" column="type"/>
        <result property="url" column="url"/>
        <result property="videoUrl" column="video_url"/>
        <result property="createTime" column="created_time" javaType="java.time.LocalDateTime" />
    </resultMap>

    <select id="getCollectPhotos" resultMap="objectMapper" parameterType="java.lang.Integer">
        SELECT *
        FROM t_client_collect_photos
        WHERE collect_id=#{collectId} and status = 0 order by sort 
    </select>


</mapper>